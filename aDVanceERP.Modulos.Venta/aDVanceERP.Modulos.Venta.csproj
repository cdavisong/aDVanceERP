    </Compile>
    <Compile Update="Vistas\VistaTuplaCliente.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Vistas\VistaTuplaMensajero.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Vistas\VistaTuplaPedido.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Vistas\VistaTuplaProductoVenta.cs">
      <SubType>Form</SubType>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>
